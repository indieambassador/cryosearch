<%= simple_form_for(@protocol) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :genus %>
    <%= f.input :species %>
    <%= f.input :cell_or_tissue_type %>
    <%= f.input :description %>
    <%= f.input :user_id, :as => :hidden, :input_html =>  {:value => current_user.id} %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
